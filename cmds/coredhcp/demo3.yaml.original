config:
    vlan200:
        server4:
            listen:
                - "%vlan200"
            plugins:
                - lease_time: 3600s
                - server_id: 192.168.1.10
                - file: file_leases_vlan200.txt
                - dns: 192.168.1.10
                - mtu: 1500
                - searchdomains:
                      - cloud.local
                - router: 192.168.200.1
                - netmask: 255.255.255.255
                - range: leases_vlan200.txt 192.168.200.10 192.168.200.50 60s
                - staticroute: 10.20.20.0/24,192.168.200.1 0.0.0.0/0,192.168.200.1
    vlan400:
        server4:
            listen:
                - "%vlan400"

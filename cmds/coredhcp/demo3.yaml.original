                - netmask: 255.255.255.255
                - range: leases_vlan400.txt 192.168.201.10 192.168.201.50 60s
                - staticroute: 10.20.21.0/24,192.168.201.1 192.168.201.0/24,192.168.201.1
    vlan600:
        server4:
